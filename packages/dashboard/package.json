{
  "name": "@evan.network/sidetree-dashboard",
  "version": "0.3.1-unstable.62",
  "private": true,
  "scripts": {
    "predev:docker": "docker-compose -f ./docker-compose.build.yml build",
    "dev:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.example:sidetree.testnet up",
    "predev:ganache:docker": "docker-compose -f ./docker-compose.build.yml build --no-cache",
    "dev:ganache:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.ganache up",
    "predev:photon:docker": "docker-compose -f ./docker-compose.build.yml build",
    "dev:photon:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.photon up",
    "predev:ropsten:docker": "docker-compose -f ./docker-compose.build.yml build",
    "dev:ropsten:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.ropsten up",
    "dev": "dotenv -e .env.example:sidetree.testnet next dev",
    "dev:ganache": "NODE_OPTIONS=--max_old_space_size=4096 dotenv -e .env.ganache next dev",
    "dev:photon": "dotenv -e .env.photon next dev",
    "dev:ropsten": "NODE_OPTIONS=--max_old_space_size=4096 dotenv -e .env.ropsten next dev",
    "test": "jest",
    "build": "npx next build",
    "export": "next export",
    "start": "next start -p ${PORT:=3000}",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@evan.network/sidetree-cas": "^0.3.1-unstable.70",
    "@evan.network/sidetree-cas-ipfs": "^0.3.1-unstable.70",
    "@evan.network/sidetree-common": "^0.3.1-unstable.70",
    "@evan.network/sidetree-core": "^0.3.1-unstable.70",
    "@evan.network/sidetree-crypto": "^0.3.1-unstable.70",
    "@evan.network/sidetree-db": "^0.3.1-unstable.70",
    "@evan.network/sidetree-did-method": "^0.3.1-unstable.70",
    "@evan.network/sidetree-element": "^0.3.1-unstable.71",
    "@evan.network/sidetree-ethereum": "^0.3.1-unstable.71",
    "@evan.network/sidetree-photon": "^0.3.1-unstable.70",
    "@evan.network/sidetree-wallet": "^0.3.1-unstable.70",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@mui/styles": "^5.11.12",
    "@mui/x-data-grid": "^6.0.1",
    "@transmute/universal-wallet": "^0.7.0-unstable.51",
    "aws-sdk": "^2.1332.0",
    "core-js": "^3.29.0",
    "express-oauth2-jwt-bearer": "^1.3.0",
    "factory.ts": "^1.3.0",
    "jose": "^4.13.1",
    "moment": "^2.29.4",
    "next": "^13.2.3",
    "react": "18.2.0",
    "react-c3js": "^0.1.20",
    "react-dom": "18.2.0",
    "react-tsparticles": "^2.9.3",
    "web3": "^1.8.2"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "dotenv-cli": "^7.0.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "events": "^3.3.0",
    "jest": "^29.5.0",
    "typescript": "^4.9.5"
  }
}
